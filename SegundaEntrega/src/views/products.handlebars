<div>
  {{#each docs}}
    <div>
      <div>
        <h4>{{this.title}}</h4>
        <p>{{this.description}}</p>
        <p>${{this.price}}</p>
      </div>
      <div>
        <button id={{this._id}}>Add to cart</button>
      </div>
    </div>
  {{/each}}
  <div>
    {{#if hasPrevPage}}
      <a href={{prevLink}}>Anterior</a>
    {{/if}}
    {{#if hasNextPage}}
      <a href={{nextLink}}>Siguiente</a>
    {{/if}}
  </div>
</div>

<script src="products.js"></script>